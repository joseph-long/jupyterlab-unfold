{
  "name": "jupyterlab-unfold-ui-tests",
  "version": "1.0.0",
  "description": "JupyterLab jupyterlab-unfold Integration Tests",
  "private": true,
  "scripts": {
    "bench:filebrowser": "jlpm playwright test --config=playwright.benchmark.config.js",
    "prepare:benchmark-data": "node scripts/create-benchmark-dataset.js",
    "start": "jupyter lab test_structure --config jupyter_server_test_config.py",
    "start:bench": "jlpm prepare:benchmark-data && jlpm start",
    "test": "jlpm playwright test",
    "test:update": "jlpm playwright test --update-snapshots"
  },
  "devDependencies": {
    "@jupyterlab/galata": "^5.0.5",
    "@playwright/test": "^1.37.0"
  }
}
